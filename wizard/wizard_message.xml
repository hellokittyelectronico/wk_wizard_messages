<odoo>
	<data>
		<record id="form_wizard_message" model="ir.ui.view">
			<field name="name">Message wizard</field>
			<field name="model">wk.wizard.message</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Message">
					<separator string="Message" colspan="6" />
					<field name="text" colspan="4" nolabel="1" readonly="1" widget="html" />
					<separator string="Como solucionarlo" colspan="6" />
                    <field name="text2" colspan="4" nolabel="1" readonly="1" widget="url" />
                    <newline/>
                    <separator colspan="6" />
                    <footer>
                        <button special="cancel" class="oe_highlight" string="Cerrar" />
                        <a href="https://navegasoft.com/im_livechat/support/1" target="_blank">-<button class="oe_highlight" string="Chat de soporte" /></a>
                    </footer>
				</form>
			</field>
		</record>
	</data>
</odoo>
